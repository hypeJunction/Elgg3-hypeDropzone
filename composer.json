{
    "name": "hypejunction/hypedropzone",
    "version": "6.1.1",
    "type": "elgg-plugin",
    "description": "Drag-n-drop file uploads for Elgg",
    "homepage": "http://hypejunction.com",
    "keywords": [
        "elgg",
        "plugin",
        "upload",
        "dropzone",
        "drag-n-drop"
    ],
    "license": "proprietary",
    "authors": [
        {
            "name": "Ismayil Khayredinov",
            "email": "info@hypejunction.com"
        }
    ],
    "extra": {
        "installer-name": "hypeDropzone"
    },
    "require": {
        "php": ">=5.5",
        "composer/installers": "~1.0",
        "npm-asset/dropzone": "<=5.4.0 || >5.5.0"
    },
    "autoload": {
        "psr-0": {
            "": "classes/"
        }
    },
    "config": {
        "fxp-asset": {
            "enabled": false
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
